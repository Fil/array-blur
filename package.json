  "keywords": [
    "d3",
    "d3-module",
    "blur",
    "array"
  ],
  "homepage": "https://github.com/fil/array-blur",
  "license": "BSD-3-Clause",
  "author": {
    "name": "Fil",
    "url": "https://github.com/fil"
  },
  "main": "dist/array-blur.js",
  "unpkg": "dist/array-blur.min.js",
  "module": "src/index.js",
